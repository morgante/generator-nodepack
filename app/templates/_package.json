{
  "name": "<%= slugname %>",
  "version": "0.0.0",
  "main": "lib/<%= slugname %>.js",
  "description": "<%= props.description %>",
  <%
  if (props.homepage) { %>
      "homepage": "<%= props.homepage %>",
    <%
  } %>
    "bugs": "<%= repoUrl %>/issues",
  "author": {
    "name": "<%= props.authorName %>",
    "email": "<%= props.authorEmail %>" <%
    if (props.authorUrl) { %> ,
        "url": "<%= props.authorUrl %>" <%
    } %>
  },
  "repository": {
    "type": "git",
    "url": "<%= repoUrl %>"
  },
  "license": "<%= props.license %>",
  "files": [
    "lib"
  ],
  "keywords": [
    "<%= slugname %>" <%
    for (var i = 0; i < keywords.length; i++) { %> ,
        "<%= keywords[i] %>" <%
    } %>
  ],
  "devDependencies": {
    "jshint-stylish": "^0.2.0" <%
    if (props.browser === "yes") { %> ,
        "browserify": "^4.1.3" <%
    } %>
  },
  "scripts": {
    <%
    if (props.browser === "yes") { %>
        "browser": "browserify lib/<%= slugname %>.js > browser.js" <%
    } %>
  } <%
  if (props.cli === "yes") { %> ,
      "bin": {
        "<%= slugname %>": "cli.js"
    },
    "preferGlobal": "true" <%
  } %>
}
